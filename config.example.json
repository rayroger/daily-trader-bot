  "data_source": {
    "type": "yahoo_finance",
    "lookback_days": 60
  },
  "model": {
    "n_estimators": 100,
    "max_depth": 10,
    "random_state": 42
  },
  "ai_provider": {
    "type": "openai",
    "model": "gpt-3.5-turbo"
  }
}
